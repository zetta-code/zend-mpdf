{
  "name": "zetta-code/zend-mpdf",
  "type": "library",
  "description": "Suporte a utilização MPDF no zend Framework 3.",
  "license": "MIT",
  "keywords": [
    "pdf",
    "mpdf",
    "zf3"
  ],
  "homepage": "https://github.com/zetta-code/zend-mpdf",
  "authors": [
    {
      "name": "Thiago S. Santos",
      "email": "thiagos.xsantos@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "laminas/laminas-stdlib": "^3.4",
    "mpdf/mpdf": "^8.0",
    "ocramius/proxy-manager": "^2.10",
    "laminas/laminas-dependency-plugin": "^2.1"
  },
  "require-dev": {
    "laminas/laminas-mvc": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Zetta\\ZendMPDF\\": "src/"
    }
  },
  "extra": {
    "laminas": {
      "component": "Zetta\\ZendMPDF",
      "config-provider": "Zetta\\ZendMPDF\\ConfigProvider"
    }
  }
}